#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,m,sum=0;
    scanf("%d\n",&n);
    scanf("%d\n",&m);
    if(n<=0||n>=30||m<=0||m>=30||n>m){
        printf("Invalid Input\n");
        return 0;
    }
    for(int i=n;i<=m;i++){
        if(i%2==0){
            sum+=i;
    
        }
    }
    printf("%d\n",sum);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
