#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,num;
    int evencount=0,oddcount=0;
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        scanf("%d",&num);
    if(num%2==0)
        evencount++;
    else
        oddcount++;
}
    if(evencount==n)
        printf("Even\n");
    else if(oddcount==n)
        printf("Odd\n");
    else
        printf("Mixed\n");


    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
