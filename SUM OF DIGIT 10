#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,digit,sum=0;
    scanf("%d",&n);
    if(n>=100)
    {
        while(n!=0)
        {
            digit=n%10;
            sum+=digit;
            n=n/10;}
            printf("Sum of digit is %d",sum);

        }
        else
            printf("Invalid Input");
        
    
       
    return 0;
}
